{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "jasmine": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "no-unused-vars": ["warn", { "vars": "all", "args": "after-used" }]
    }
}